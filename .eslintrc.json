{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "babelOptions": {
            "presets": ["@babel/preset-react"]
         },
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module",
        "requireConfigFile": false
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": "warn",
        "require-jsdoc":"off",
        "react/prop-types": "off"
    }
}
